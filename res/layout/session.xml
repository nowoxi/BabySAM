<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@android:color/white">
<TextView  
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:text="@string/session"
    android:gravity="center_vertical|center_horizontal" android:textSize="@dimen/menu_item_med"/>
<TableLayout android:layout_width="fill_parent" android:id="@+id/tableLayout1" android:layout_height="fill_parent" android:layout_marginTop="@dimen/vspace">
    <TableRow android:layout_width="wrap_content" android:gravity="center_vertical|center_horizontal" android:id="@+id/TableRow01" android:layout_height="wrap_content">
        <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TextView01" android:text="TextView"></TextView>
        <EditText android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/EditText01" android:text="EditText"></EditText>
    </TableRow>
    <TableRow android:layout_width="wrap_content" android:gravity="center_vertical|center_horizontal" android:id="@+id/TableRow02" android:layout_height="wrap_content">
        <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TextView02" android:text="TextView"></TextView>
        <EditText android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/EditText02" android:text="EditText"></EditText>
    </TableRow>
    <TableRow android:layout_width="wrap_content" android:gravity="center_vertical|center_horizontal" android:id="@+id/TableRow03" android:layout_height="wrap_content">
        <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TextView03" android:text="TextView"></TextView>
        <EditText android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/EditText03" android:text="EditText"></EditText>
    </TableRow>
    <TableRow android:layout_width="wrap_content" android:gravity="center_vertical|center_horizontal" android:id="@+id/TableRow04" android:layout_height="wrap_content">
        <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TextView04" android:text="TextView"></TextView>
        <EditText android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/EditText04" android:text="EditText"></EditText>
    </TableRow>
</TableLayout>
</LinearLayout>
